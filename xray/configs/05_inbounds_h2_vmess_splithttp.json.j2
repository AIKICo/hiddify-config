{
	{% if 0 and hconfigs['vmess_enable'] and hconfigs['xhttp_enable'] %}
		"inbounds": [
			{
				"tag":"vmess-xhttp-h2",
				{# "listen": "@@vmess-xhttp-h2",#}
				"listen": "127.0.0.1", "port":1036,
				"protocol": "vmess",
				"settings": {
				"clients": [
					{% for u in users %}
						{"id": "{{ u['uuid'] }}", "email": "{{ u['uuid'] }}@hiddify.com", "alterId": 0},
					{% endfor %}
				]
				},
				"streamSettings": {
				"network": "xhttp",
				"security": "none",
				"xhttpSettings": {
					{#"acceptProxyProtocol": true,#}
					"path": "/{{ hconfigs['path_vmess'] }}{{ hconfigs['path_xhttp'] }}2", 
				}
				},
				"sniffing": {
				"enabled": true,
				"destOverride": ["http","tls", "quic"]
				}
			}	  
		]
	{% endif %}
}